table.center, td.center
  text-align center

h1.center,
h2.center,
h3.center,
h4.center,
h5.center,
h6.center
  text-align center

span
  &.center
    display block
    width 100%
    text-align center

.show-for-small,
.hide-for-desktop
  display none

.panel
  background #f2f2f2
  border 1px solid #d9d9d9
  padding 10px !important

.sub-grid
  table
    width 100%
  td
    &.sub-columns
      padding-bottom 0

table.button,
table.tiny-button,
table.small-button,
table.medium-button,
table.large-button
