  &.small-button
    td
      padding 8px 0 7px
  &.medium-button
    td
      padding 12px 0 10px
  &.large-button
    td
      padding 21px 0 18px

td
  word-break break-word
  -webkit-hyphens auto
  -moz-hyphens auto
  hyphens auto
  border-collapse collapse !important
  &.wrapper
    padding 10px 20px 0px 0px
    position relative
  &.offset-by-one
    padding-left 50px
  &.offset-by-two
    padding-left 100px
  &.offset-by-three
    padding-left 150px
  &.offset-by-four
    padding-left 200px
